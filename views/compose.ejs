<%- include("partials/header") -%>
<h1>Compose</h1>
<form action="/compose" method="post">
  <div class="mb-3">
    <label for="postTitle" class="form-label">Title</label>
    <input type="text" name="postTitle" id="postTitle" class="form-control" />
  </div>
  <div class="mb-3">
    <label for="postContent" class="form-label">Post</label>
    <textarea
      name="postContent"
      id="postContent"
      cols="30"
      rows="10"
      class="form-control"
    ></textarea>
  </div>
  <button type="submit" name="button" class="btn btn-primary">Publish</button>
</form>
<%- include("partials/footer") -%>
